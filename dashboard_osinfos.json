{
	"version": 1,
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"title": "CPU",
			"width": 1,
			"row": {
				"3": 1
			},
			"col": {
				"3": 2
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "plot",
					"settings": {
						"title": "CPU Usage (%)",
						"xaxis": "seconds_from_start",
						"time_window": "60",
						"value": [
							"datasources[\"osinfos\"][\"cpuUsage\"]"
						],
						"y_axis_min_range": "0,100",
						"y2_axis_min_range": "-10,10",
						"height": 3,
						"pausable": false
					}
				},
				{
					"type": "text_widget",
					"settings": {
						"title": "CPU Usage",
						"size": "regular",
						"value": "datasources[\"osinfos\"][\"cpuUsage\"]",
						"animate": false,
						"units": "%"
					}
				}
			]
		},
		{
			"title": "System",
			"width": 1,
			"row": {
				"3": 1
			},
			"col": {
				"3": 1
			},
