					}
				},
				{
					"type": "text_widget",
					"settings": {
						"title": "Process Uptime",
						"size": "regular",
						"value": "datasources[\"osinfos\"][\"processUptime\"]",
						"animate": false,
						"units": "s"
					}
				}
			]
